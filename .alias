# git alias
alias o='git fetch origin'
alias s='git status'
alias a='git add'
alias d='git diff -w'
alias dm='git diff origin/master'
alias dmf='git diff origin/master --name-only'
alias b='git branch'
alias cm='git checkout master'
alias c='git checkout'
alias ggpush='git push'
alias ggpull='git pull'
alias rc='git commit -m "$(git status --porcelain)"'
alias m='git merge origin/master'
alias rb='git fetch origin && git rebase -i origin/master'

# shortcuts
alias ll='ls -lh'
alias ws='cd $HOME/Documents/workspace/'
alias ek='cd $HOME/Documents/workspace/ekanek'
alias hg='cd $HOME/Documents/workspace/hitesh-goel/'

# services
alias md='make dc-down'
alias mu='make dc-up'
alias pg_start='brew services start postgresql'
alias pg_stop='brew services stop postgresql'
alias pg_restart='brew services restart postgresql'
alias rd_start='brew services start redis'
alias rd_stop='brew services stop redis'
alias sidekiq-co='cd $HOME/Documents/workspace/ekanek/cat_ops/ && bundle exec sidekiq -q cat-ops-offer'
alias rs='./bin/rails server'
alias rc='./bin/rails console'
alias bs='brew services --all'
